#constants
var NO_DATA = -20000

#variables
seq good[]
var data

#functions
fun InArray(loc, arr[])
    for tile in arr[]
        if tile is loc
            return 1
    return 0

fun WaitData()
    data = NO_DATA
    while data is NO_DATA
        data = receive(0)

#main
loop
    WaitData()
    if data is 0
        break
    good[].add(data)
loop
    if InArray(robo_forward_location(), good[])
        robo_forward()
    else
        robo_left()
